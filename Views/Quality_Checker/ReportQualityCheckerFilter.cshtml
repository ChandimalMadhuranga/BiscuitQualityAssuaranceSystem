@model BiscuitQualityAssuaranceSystem.Models.Quality_Checker

@{
    ViewBag.Title = "ReportQualityChecker";
}


    @using (Html.BeginForm("ReportQualityChecker", "Quality_Checker", FormMethod.Post))
    {
<div>
    <br /><br />
    <h4>Quality Checker Details Report</h4>

    <p>
        <div class="form-group">
            @Html.LabelFor(model => model.SM_ID, "Shift Manager", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("SM_ID", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.SM_ID, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Generate Report" class="btn btn-warning" />
        </div>
    </p>
</div>
    }





